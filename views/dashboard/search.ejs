<div class="container-fluid container-fluid-custom pb-5 mb-5">
  <div class="row col-cols-2 col-cols-lg-5 g-2 g-lg-3 mx-4">
    <% if (searchResults != '') { %> <% searchResults.forEach(function
    (note,index) { %>
    <a href="/dashboard/item/<%= note._id %>">
      <h4><%= note.title %></h4>
    </a>
    <% }) %> <% } else { %>
    <p>Nothing Found</p>
    <% } %>
  </div>
</div>
